# Exclude development and testing files from npm package

# Source TypeScript files (compiled .js files will be included instead)
*.ts
!*.d.ts

# TypeScript config
tsconfig.json

# Environment files (example should be included)
.env
.env.local
.env.*.local

# Node modules
node_modules/

# Test files
*.test.ts
*.spec.ts
__tests__/
coverage/

# Build artifacts
dist/
build/
*.tsbuildinfo

# IDE and editor files
.vscode/
.idea/
*.swp
*.swo
*~

# OS files
.DS_Store
Thumbs.db

# Git files
.git/
.gitignore
